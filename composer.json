{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "doctrine/dbal": "^3.3.3",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.2.0",
        "guzzlehttp/guzzle": "^7.4.1",
        "laratips/filterable": "dev-main",
        "laratips/sortable": "dev-main",
        "laravel/framework": "^8.83.4",
        "laravel/tinker": "^2.7.0",
        "predis/predis": "^2.2",
        "symfony/css-selector": "^6.0.3",
        "symfony/dom-crawler": "^6.0.6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "facade/ignition": "^2.17.5",
        "fakerphp/faker": "^1.19.0",
        "laravel/sail": "^1.13.6",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^5.11.0",
        "phpunit/phpunit": "^9.5.18"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/laratips/sortable"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laratips/filterable"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
